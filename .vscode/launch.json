    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Step 1: Download Products JSON",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
            ]
        },
        {
            "name": "Step 2: Ingest Products",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ingest_products.py",
            "console": "integratedTerminal",
            "args": [
                "--file",
                "${workspaceFolder}/all_products_all_data.json"
            ]
        }
    ]
}